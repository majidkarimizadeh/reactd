    "html-react-parser": "^0.4.7",
    "leaflet": "^1.4.0",
    "moment": "^2.22.2",
    "moment-jalaali": "^0.8.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "primeflex": "1.0.0-rc.1",
    "primeicons": "1.0.0-beta.10",
    "primereact": "2.0.0-beta.8",
    "quill": "^1.3.1",
    "react": "^16.6.3",
    "react-advance-jalaali-datepicker": "^1.0.0",
    "react-dom": "^16.6.3",
    "react-froala-wysiwyg": "^2.9.1",
    "react-image-crop": "^6.0.9",
